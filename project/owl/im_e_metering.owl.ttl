@prefix PATO: <http://purl.obolibrary.org/obo/PATO_> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix im_e_metering: <https://w3id.org/Netbeheer-Nederland/im-e-metering/> .
@prefix linkml: <https://w3id.org/linkml/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema1: <http://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

im_e_metering:IdentifiedObjectCollection a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "IdentifiedObjectCollection" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom im_e_metering:IdentifiedObject ;
            owl:onProperty im_e_metering:entries ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty im_e_metering:entries ] ;
    skos:definition "A holder for IdentifiedObject objects" ;
    skos:inScheme <https://w3id.org/Netbeheer-Nederland/im-e-metering> .

im_e_metering:IdentifiedObject a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "IdentifiedObject" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty im_e_metering:vital_status ],
        [ a owl:Restriction ;
            owl:allValuesFrom im_e_metering:PersonStatus ;
            owl:onProperty im_e_metering:vital_status ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty im_e_metering:age_in_years ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty im_e_metering:primary_email ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Date ;
            owl:onProperty im_e_metering:birth_date ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty im_e_metering:birth_date ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty im_e_metering:birth_date ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty im_e_metering:vital_status ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:onDatatype xsd:string ;
                    owl:withRestrictions ( [ xsd:pattern "^\\S+@[\\S+\\.]+\\S+" ] ) ] ;
            owl:onProperty im_e_metering:primary_email ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty im_e_metering:primary_email ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty im_e_metering:age_in_years ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Integer ;
            owl:onProperty im_e_metering:age_in_years ],
        im_e_metering:NamedThing ;
    skos:definition "Represents a IdentifiedObject" ;
    skos:inScheme <https://w3id.org/Netbeheer-Nederland/im-e-metering> .

im_e_metering:NamedThing a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "NamedThing" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty im_e_metering:id ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty im_e_metering:id ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty im_e_metering:description ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty im_e_metering:description ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty im_e_metering:description ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty im_e_metering:name ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty im_e_metering:name ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Uriorcurie ;
            owl:onProperty im_e_metering:id ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty im_e_metering:name ] ;
    skos:definition "A generic grouping for any identifiable entity" ;
    skos:exactMatch schema1:Thing ;
    skos:inScheme <https://w3id.org/Netbeheer-Nederland/im-e-metering> .

PATO:0001421 a owl:Class,
        im_e_metering:PersonStatus ;
    rdfs:label "ALIVE" ;
    rdfs:subClassOf im_e_metering:PersonStatus .

PATO:0001422 a owl:Class,
        im_e_metering:PersonStatus ;
    rdfs:label "DEAD" ;
    rdfs:subClassOf im_e_metering:PersonStatus .

<https://w3id.org/Netbeheer-Nederland/im-e-metering/PersonStatus#UNKNOWN> a owl:Class,
        im_e_metering:PersonStatus ;
    rdfs:label "UNKNOWN" ;
    rdfs:subClassOf im_e_metering:PersonStatus .

im_e_metering:entries a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "entries" ;
    skos:inScheme <https://w3id.org/Netbeheer-Nederland/im-e-metering> .

im_e_metering:age_in_years a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "age_in_years" ;
    rdfs:range linkml:Integer ;
    skos:definition "Number of years since birth" ;
    skos:inScheme <https://w3id.org/Netbeheer-Nederland/im-e-metering> .

im_e_metering:birth_date a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "birth_date" ;
    rdfs:range linkml:Date ;
    skos:definition "Date on which a person is born" ;
    skos:inScheme <https://w3id.org/Netbeheer-Nederland/im-e-metering> .

im_e_metering:description a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "description" ;
    skos:definition "A human-readable description for a thing" ;
    skos:inScheme <https://w3id.org/Netbeheer-Nederland/im-e-metering> .

im_e_metering:id a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "id" ;
    rdfs:range linkml:Uriorcurie ;
    skos:definition "A unique identifier for a thing" ;
    skos:inScheme <https://w3id.org/Netbeheer-Nederland/im-e-metering> .

im_e_metering:name a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "name" ;
    skos:definition "A human-readable name for a thing" ;
    skos:inScheme <https://w3id.org/Netbeheer-Nederland/im-e-metering> .

im_e_metering:primary_email a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "primary_email" ;
    skos:definition "The main email address of a person" ;
    skos:inScheme <https://w3id.org/Netbeheer-Nederland/im-e-metering> .

im_e_metering:vital_status a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "vital_status" ;
    rdfs:range im_e_metering:PersonStatus ;
    skos:definition "living or dead status" ;
    skos:inScheme <https://w3id.org/Netbeheer-Nederland/im-e-metering> .

im_e_metering:PersonStatus a owl:Class,
        linkml:EnumDefinition ;
    owl:unionOf ( PATO:0001421 PATO:0001422 <https://w3id.org/Netbeheer-Nederland/im-e-metering/PersonStatus#UNKNOWN> ) ;
    linkml:permissible_values PATO:0001421,
        PATO:0001422,
        <https://w3id.org/Netbeheer-Nederland/im-e-metering/PersonStatus#UNKNOWN> .

<https://w3id.org/Netbeheer-Nederland/im-e-metering> a owl:Ontology ;
    rdfs:label "im-e-metering" ;
    dcterms:license "Apache Software License 2.0" ;
    dcterms:title "im-e-metering" ;
    rdfs:seeAlso <https://Netbeheer-Nederland.github.io/im-e-metering> ;
    skos:definition "Informatiemodel voor het NBNLES Metering Profile." .

